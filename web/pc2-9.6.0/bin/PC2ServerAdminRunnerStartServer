#!/bin/sh

cd "$(dirname "$0")"
chmod +x pc2server
chmod +x pc2admin
chmod +x pc2judge
chmod +x pc2board
chmod +x pc2team
./pc2server --login $1 --password $2 --contestpassword $3 2>&1 &
sleep 4
./pc2admin --login $4 --password $5 2>&1 &
./pc2judge --login $6 --password $7 2>&1 &
./pc2board --login $8 --password $9 2>&1 &
./pc2team --login ${10} --password ${11} 2>&1 &